<window>
	<name>File window</name>
	<w>1</w>
	<h>1</h>
	<windowborder>
		<x>-1</x>
		<y>.8</y>
		<w>2</w>
		<h>.2</h>
		<exitbutton>
			<x>.9</x>
			<y>-1</y>
			<w>.1</w>
			<h>2</h>
		</exitbutton>
	</windowborder>
	<menu>
		<name>filewindowmenu</name>
		<x>-.99</x>
		<y>.7</y>
		<w>1.98</w>
		<h>.1</h>
		<menuitem>
			<name>back</name>
			<label>back</label>
			<x>-1</x>
			<y>-1</y>
			<w>.2</w>
			<h>2</h>
			<action>self.makegoback(button,state,x,y)</action>
		</menuitem>
		<menuitem>
			<name>up</name>
			<label>up</label>
			<x>-.8</x>
			<y>-1</y>
			<w>.1</w>
			<h>2</h>
			<action>self.makegoup(button,state,x,y)</action>
		</menuitem>
	</menu>
	<textbox>
		<name>addressbox</name>
		<editable>False</editable>
		<text>text</text>
		<x>-.99</x>
		<y>.6</y>
		<w>1.98</w>
		<h>.1</h>
	</textbox>
	<fileview>
		<x>-.99</x>
		<y>-.99</y>
		<w>1.98</w>
		<h>1.59</h>
	</fileview>
</window>
